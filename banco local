import sqlite3

# conectando...
conn = sqlite3.connect('clientes.db')
# definindo um cursor
cursor = conn.cursor()

# criando a tabela (schema)
cursor.execute("""CREATE TABLE tabe (
    IDUSU  INTEGER      PRIMARY KEY AUTOINCREMENT
                        NOT NULL,
    Nnome  TEXT NOT NULL,
    Ccpf   VARCHAR (11) NOT NULL,
    Nnumr  VARCHAR (9)  NOT NULL,
    Ccep   VARCHAR (8)  NOT NULL,
    Ssenha VARCHAR (20) NOT NULL
);""")
print('Tabela criada com sucesso.')
# desconectando...
conn.close()